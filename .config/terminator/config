[global_config]
  suppress_multiple_term_dialog = True
[keybindings]
  split_horiz = <Shift><Super>d
  split_vert = <Super>d
  close_term = <Primary>w
  move_tab_right = <Primary><Shift>Right
  move_tab_left = <Primary><Shift>Left
  next_tab = <Shift><Alt>Right
  prev_tab = <Shift><Alt>Left
[profiles]
  [[default]]
    background_color = "#002b36"
    cursor_color = "#aaaaaa"
    foreground_color = "#839496"
    scrollback_infinite = True
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
